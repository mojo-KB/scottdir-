function doclick2() {
  x = document.getElementById("mybutton4");
  if (x.value == "Show Faces") {
        x.value = "Hide Faces";
        for (var i = 0; i < faceData.length; i++) {
                var counter = faceData[i];
                document.getElementById("faces").innerHTML += counter + "--"; 
        }
  }
  else {
        x.value = "Show Faces";
        document.getElementById("faces").innerHTML = "";
  }
}
